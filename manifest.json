{
  "manifest_version": 3,
  "name": "Bookmark Click Tracker",
  "version": "1.0",
  "description": "Counts the number of times bookmarks are clicked.",
  "permissions": [
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "history"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}