{
  "manifest_version": 2,
  "name": "Bookmark Click Tracker",
  "version": "1.0",
  "description": "Counts the number of times bookmarks are clicked.",
  "permissions": ["tabs", "bookmarks", "storage", "webNavigation","history"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
