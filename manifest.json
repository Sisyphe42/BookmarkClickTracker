{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "2.0",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "permissions": [
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "history"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
